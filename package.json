{
  "name": "microservice-starter",
  "version": "1.0.0",
  "description": "Microservice starter with built in Liquid integration.",
  "main": "index.js",
  "browserslist": "> 0.25%, not dead",
  "scripts": {
    "test": "mocha -r ts-node/register 'test/integration/**/*.spec.ts' --exit --recursive --require test/integration/index.spec.ts",
    "build": "rimraf ./build && tsc && npm run copy-files",
    "copy-files": "copyfiles -u 1 \"src/**/*.yaml\" \"src/**/*.html\" \"src/**/*.json\" \"src/VERSION\" build/",
    "start": "nodemon --inspect",
    "pretty": "prettier --write \"./**/*.{js,ts,json}\""
  },
  "keywords": [],
  "author": "Shrihari Prakash",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/chai-http": "^4.2.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.5",
    "@types/express-validator": "^3.0.0",
    "@types/mocha": "^10.0.1",
    "@types/multer": "^1.4.7",
    "@types/multer-s3": "^3.0.0",
    "@types/node": "^18.7.23",
    "@types/nodemailer": "^6.4.11",
    "@types/uuid": "^9.0.1",
    "chai": "^4.3.7",
    "chai-http": "^4.3.0",
    "copyfiles": "^2.4.1",
    "del": "^6.1.1",
    "mocha": "^10.2.0",
    "mongodb-memory-server": "^8.12.2",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.405.0",
    "@aws-sdk/s3-request-presigner": "^3.405.0",
    "@sendgrid/mail": "^8.1.0",
    "body-parser": "^1.20.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.1",
    "express-rate-limit": "^6.7.0",
    "express-validator": "^6.14.2",
    "ioredis": "^5.2.4",
    "liquid-node-connector": "^1.0.13",
    "moment": "^2.29.4",
